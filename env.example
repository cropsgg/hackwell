# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/wellness

# JWT Configuration
JWT_AUDIENCE=authenticated
JWT_ISSUER=https://your-project.supabase.co/auth/v1

# External APIs
PUBMED_BASE=https://eutils.ncbi.nlm.nih.gov/entrez/eutils
OPENFDA_BASE=https://api.fda.gov
RXNORM_BASE=https://rxnav.nlm.nih.gov/REST

# Evidence Verification
EVIDENCE_MIN_SCORE=0.6

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# Model Configuration
ML_MODEL_PATH=services/ml_risk/models
DEFAULT_MODEL_VERSION=risk_lgbm_v0.1

# Security
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080
